<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sangui.sanguimall.admin.mapper.QrtzLocksMapper">
  <resultMap id="BaseResultMap" type="com.sangui.sanguimall.admin.model.entity.QrtzLocksKey">
    <id column="SCHED_NAME" jdbcType="VARCHAR" property="schedName" />
    <id column="LOCK_NAME" jdbcType="VARCHAR" property="lockName" />
  </resultMap>
  <delete id="deleteByPrimaryKey" parameterType="com.sangui.sanguimall.admin.model.entity.QrtzLocksKey">
    delete from qrtz_locks
    where SCHED_NAME = #{schedName,jdbcType=VARCHAR}
      and LOCK_NAME = #{lockName,jdbcType=VARCHAR}
  </delete>
  <insert id="insert" keyColumn="SCHED_NAME" keyProperty="schedName" parameterType="com.sangui.sanguimall.admin.model.entity.QrtzLocksKey" useGeneratedKeys="true">
    insert into qrtz_locks (LOCK_NAME)
    values (#{lockName,jdbcType=VARCHAR})
  </insert>
  <insert id="insertSelective" keyColumn="SCHED_NAME" keyProperty="schedName" parameterType="com.sangui.sanguimall.admin.model.entity.QrtzLocksKey" useGeneratedKeys="true">
    insert into qrtz_locks
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="lockName != null">
        LOCK_NAME,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="lockName != null">
        #{lockName,jdbcType=VARCHAR},
      </if>
    </trim>
  </insert>
</mapper>